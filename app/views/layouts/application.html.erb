<!DOCTYPE html>
<html>
<head>
  <%= stylesheet_link_tag 'application' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="header">
    <%= render partial: '/layouts/navbar' %>

  </div>


  <% flash.each do |key, value| -%>

    <div class="<%= flash_class(key) %>"> <%= value %> </div>
  <% end %>

  <div class="container">
    <%= yield %>
  </div>
</body>
</html>
